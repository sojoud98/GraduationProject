<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.grduationproject"
    android:allowBackup="true"
    android:fullBackupContent="@xml/backup_descriptor">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GrduationProject"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity_apply"></activity>
        <activity android:name=".Activity_Customer_info" />
        <activity android:name=".ActivityAdminHome" />
        <activity android:name=".ActivityCooking" />
        <activity android:name=".ActivityCleaning" />
        <activity android:name=".ActivityHome" />
        <activity android:name=".Activity_profile" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>